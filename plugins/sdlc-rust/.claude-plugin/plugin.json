{
  "name": "sdlc-rust",
  "description": "Rust domain modeling agent with cargo-mcp integration. Implements type-first TDD using nutype, Parse Don't Validate philosophy, and compiler-driven type creation for maximum compile-time safety.",
  "version": "1.0.0",
  "author": {
    "name": "John Wilger",
    "email": "john@johnwilger.com"
  },
  "homepage": "https://github.com/jwilger/claude-code-setup",
  "repository": "https://github.com/jwilger/claude-code-setup.git",
  "license": "MIT",
  "keywords": [
    "rust",
    "domain-modeling",
    "cargo",
    "mcp",
    "nutype",
    "type-first",
    "parse-dont-validate"
  ],
  "agents": ["./agents/rust-domain-model-expert.md"],
  "commands": ["./commands/setup-cargo-mcp.md"],
  "mcpServers": {
    "cargo-mcp": {
      "command": "cargo-mcp",
      "args": ["serve"]
    }
  }
