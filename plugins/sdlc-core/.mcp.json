{
  "time": {
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-time"],
    "env": {}
  },
  "memento": {
    "command": "npx",
    "args": ["-y", "@gannonh/memento-mcp"],
    "env": {
      "MEMORY_STORAGE_TYPE": "sqlite",
      "SQLITE_DB_PATH": "${HOME}/.claude/memento.db"
    }
  }
}
